
{{#each blogs as |blog|}}
<div class="blogpost-container">
    <h5></h5> 
<div>
   <b>{{blog.title}}</b>
</div>
<div>
    {{blog.content}}
</div>
<h5>
<strong>{{user.username}}</strong>
<i> - {{format_date createdAt}}</i>
</h5>
<button id="editBtn" data-id="{{blog.id}}">View</button>
</div>
</div>
{{/each}}



<script src="/js/homePage.js"></script>